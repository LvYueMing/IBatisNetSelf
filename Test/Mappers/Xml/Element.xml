<?xml version="1.0" encoding="utf-8" ?>
<sqlMap namespace="Element" xmlns="http://ibatis.apache.org/mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" >
		<resultMaps>
	    <resultMap id="ElementResult" class="ResultMap.Element,ResultMap">
        <result property="Id" column="id" />
        <result property="Name" column="name" />
        <result property="Mean" column="mean" />
        <result property="AllowValue" column="allow_value" />
        <result property="DataType" column="data_type" />
        <result property="DataFormat" column="data_format" />
        <result property="CodeValueId" column="code_value_id" />
        <result property="Version" column="version" />
      </resultMap>
	 </resultMaps>
		<statements>
		<select id="Element_GetELement" parameterClass="Hashtable" resultMap="ElementResult">
      select a.id,
             a.name,
             a.mean,
             a.allow_value,
             a.data_type,
             a.data_format,
             a.code_value_id,
             a.max_number_value,
             a.min_number_value,
             a.unit,
             a.version,
             a.update_worker_id,
             a.update_date
        from hsdict.data_element_ a
       where a.id = #in_id#
		</select>
	</statements>


</sqlMap>
